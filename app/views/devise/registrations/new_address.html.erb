<h2>Address</h2>

<%= form_for(@address) do |f| %>
  <%= render "devise/shared/error_messages", resource: @address %>

  <div class="field">
    <%= f.label :post_family_name %><br />
    <%= f.text_field :post_family_name %>
  </div>

  <div class="field">
    <%= f.label :post_family_name_kana %><br />
    <%= f.text_field :post_family_name_kana %>
  </div>

  <div class="field">
    <%= f.label :post_first_name %><br />
    <%= f.text_field :post_first_name %>
  </div>

  <div class="field">
    <%= f.label :post_first_name_kana %><br />
    <%= f.text_field :post_first_name_kana %>
  </div>

  <div class="field">
    <%= f.label :postal_code %><br />
    <%= f.text_field :postal_code %>
  </div>

  <div class="field">
    <%= f.label :prefecture_id %><br />
    <%= f.text_field :prefecture_id %>
  </div>

  <div class="field">
    <%= f.label :city %><br />
    <%= f.text_field :city %>
  </div>

  <div class="field">
    <%= f.label :address %><br />
    <%= f.text_field :address %>
  </div>

  <div class="field">
    <%= f.label :building %><br />
    <%= f.text_field :building %>
  </div>

  <div class="field">
    <%= f.label :phone_number %><br />
    <%= f.text_field :phone_number %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
